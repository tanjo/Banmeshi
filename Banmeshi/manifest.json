{
  "manifest_version" : 2,

  "name" : "Banmeshi",
  "description" : "晩飯をランダムに表示",
  "version" : "1.0.3",

  "permissions" : [
    "http://cookpad.com/",
    "tabs"
  ],

  "icons" : {
    "128" : "128.png",
    "48" : "48.png",
    "16" : "16.png"
  },

  "background" : {
    "scripts" : ["background.js"]
  },

  "browser_action" : {
    "default_icon" : "icon.png"
  }
}
